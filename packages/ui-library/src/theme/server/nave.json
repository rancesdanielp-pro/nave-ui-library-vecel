         },
         "icon":{
            "base":{
               "color":"{semanticTokens.action.primary}"
            },
            "sizes":{
               "xs":12,
               "sm":16,
               "md":20,
               "lg":24,
               "xl":32
            },
            "variants":{
               "default":{
                  "color":"{semanticTokens.action.primary}"
               },
               "muted":{
                  "color":"{semanticTokens.text.muted}"
               },
               "danger":{
